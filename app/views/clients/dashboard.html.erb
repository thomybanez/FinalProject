Client Dashboard

<%= link_to "Profile", clients_edit_path %>
<%= link_to "Activities", bookings_show_path %>
<%= link_to "Logout", users_home_path %><br>

<%= link_to clients_edit_path(@client) do %>
    <h4>Nickname:<%= @client.nick_name %></h4>
    <p>Age: <%= @client.age %></p>
    <p>Gender: <%= @client.gender %></p>
<% end %>



<% @performer.each do |performer| %>

	<div class="container">
	<div class="box" style="--i:0;">
		<i></i>
		<div class="content">
			<% performer.photos.each do |photo| %>
			<%= image_tag photo.variant(resize_to_fill: [200, 300]) %>
				<h2><%= performer.stage_name %><br>
				<span>Age: <%= performer.age %></span><br>
				<span>Gender: <%= performer.gender %></span></h2>
				<%= link_to "Book Now", clients_show_path(performer.id) %>
			<% end %>
		</div>
	</div>
	</div>

  <% end %> 
