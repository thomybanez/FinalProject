Client Register

<%= form_with scope: @client, url: clients_create_path , method: 'post'  do |form| %>

    <%= form.label :email %>
    <%= form.text_field :email %>

    <%= form.label :password %>
    <%= form.password_field :password %>

    <%= form.label :password_confirmation %>
    <%= form.password_field :password_confirmation %>

    <%= form.submit %>

<% end %>