Performer Login

<%= form_with scope: @performer, url: performers_login_submit_path, html: {autocomplete: "off"} do |form|%>
    <%= @performer %>
        <%= form.label :email %><br>
        <%= form.text_field :email %><br>
    
        <%= form.label :password %><br>
        <%= form.password_field :password %><br>
    
        <%= form.submit "login" %>
    
    
    <% end %>